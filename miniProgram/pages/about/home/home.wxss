.UCenter-bg {
  display: absolute;
  justify-content: center;
  width: 100%;
  height: 25%;
  background-color:rgb(230, 247, 230);
}

.text-xl{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.png{
  height:70px;
  width: 70px;
  border-radius: 200%;
}
.bianji{
  margin-left:280px;
	font-size: 13px;
	color: #666;
	text-decoration: underline;
	font-weight: bold;
}

map,.mapBox{
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;  
  height: 100rpx;   
}

map,.mapBox{
  width: 750rpx;
  height: 300rpx;
}



.cu-bar .action:first-child {
	margin-left: 30rpx;
	font-size: 30rpx;
}
.cu-btn {
	position: relative;
	border: 0rpx;
	display: inline-flex;
	align-items: center;
	justify-content: center;
	box-sizing: border-box;
	padding: 0 30rpx;
	font-size: 28rpx;
	height: 64rpx;
	line-height: 1;
	text-align: center;
	text-decoration: none;
	overflow: visible;
	/* margin-left: initial; */
	transform: translate(0rpx, 0rpx);
  margin-right: initial;
  margin: 0;
}
.bg-green {
	background-color: var(--green);
	color: var(--white);
}
.shadow[class*="-green"] {
	box-shadow: var(--ShadowSize) var(--greenShadow);
}